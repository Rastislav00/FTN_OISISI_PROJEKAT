package model;

public enum Titula {
	
	BSc(1), MSc(2),mr(3),dr(4),profDr(5);
	int titula;
	
	private Titula() {}
	private Titula(int titula) {this.titula = titula;}
	private Titula(String tit) {
		if(tit.equals(values[0])) {
			titula = 1;
		}else if(tit.equals(values[1])) {
			
		}
	}
	
	private static String[] values = {"BSc","MSc","mr","dr","profDr"};
	
	public String toString(Titula titula2) {
		return values[this.ordinal()];
	}
	
	public static String[] getValues() {
		
		return values;
	}

}
